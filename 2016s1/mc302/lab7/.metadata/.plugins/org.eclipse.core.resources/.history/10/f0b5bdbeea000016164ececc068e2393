package br.unicamp.ic.mc302.onibus;

import java.util.ArrayList;

public class OnibusConvencional extends Onibus {

	public OnibusConvencional(int id) {
		super(id);

	}

	public void defineFileiras() {
		
		ArrayList<Fileira<Poltrona>> fileiras = new ArrayList<Fileira<Poltrona>>();
		
		for(int i=0; i<2; i++){
			Fileira<PoltronaEspecial> f = new Fileira<PoltronaEspecial>();
			
			fileiras.add(f);
			fileitas
		}
		
		super.setFileiras(fileiras);
	}

}
