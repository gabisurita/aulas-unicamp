package br.unicamp.ic.mc302.reservaDeQuartos;
public class QuartoSuiteLuxo extends Quarto {

	public QuartoSuiteLuxo() {
		// TODO Auto-generated constructor stub
	}

	public QuartoSuiteLuxo(int codigo, int tipo, double preco, int capacidade, String descricao) {
		super(codigo, tipo, preco, capacidade, descricao);
		// TODO Auto-generated constructor stub
	}
	
	public String toString(){	
		return super.toString() + "Tipo: Suite de Luxo\n"; 
	}
}
