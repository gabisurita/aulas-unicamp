package br.unicamp.ic.mc302.reservaDeQuartos;
public class QuartoSolteiro extends Quarto {

	public QuartoSolteiro() {
		// TODO Auto-generated constructor stub
	}

	public QuartoSolteiro(int codigo, int tipo, double preco, int capacidade, String descricao) {
		super(codigo, tipo, preco, capacidade, descricao);
		// TODO Auto-generated constructor stub
	}

	public String toString(){
		return super.toString() + "Tipo: Solteiro\n"; 
	}
}
