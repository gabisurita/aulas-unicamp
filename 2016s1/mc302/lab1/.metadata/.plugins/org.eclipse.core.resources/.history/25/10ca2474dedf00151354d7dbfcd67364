package br.unicamp.ic.mc302.meuVetor;

class MeuVetor {
	
	public String type;
	public int tam;
	public int[] val;

	public void inicializa(int chave, int valor){
		val[chave] = valor;
	}
	
	public void intercala(MeuVetor par){
		
		if(tam >= par.tam){
			
			MeuVetor inter = New MeuVetor(2*par.tam);
			for(int i=0; i<par.tam; i++){
				
			}
		}
		else{

			MeuVetor inter = New MeuVetor(2*par.tam);
			for(int i=0; i<par.tam; i++){
				
			}
			
			
		}
	}
	
    public MeuVetor(int tamanho){
  
    	tam = tamanho;
    	val = new int[tam];
    }
    
}