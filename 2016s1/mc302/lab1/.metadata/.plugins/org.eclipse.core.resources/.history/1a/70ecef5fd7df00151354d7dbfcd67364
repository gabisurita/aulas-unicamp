package br.unicamp.ic.mc302.documento;

class ExemploDocumento{ 
    static public void main(String args[ ]){ 
        
    	Documento d1, d2;
        d1 = new Documento();
        
        d1.criarDocumento("Camila",181101);
        d1.imprimir( ); // envio de mensagem para o objeto
        d1.editar(); //
    
        d2 = new Documento();
        
        d2.criarDocumento("Surita", 139095);
        d2.imprimir();
    }
}
