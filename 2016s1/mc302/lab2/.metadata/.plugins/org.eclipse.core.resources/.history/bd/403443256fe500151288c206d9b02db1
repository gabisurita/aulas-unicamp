package br.unicamp.ic.mc302.minhaMatriz;

class TestaMatriz {
	
    public static void main (String [] args) {
    	
    	int Aval[][] = {{1,2},{3,4}};
    	int Bval[][] = {{1,0},{0,1}};
    	
    	
    	MinhaMatriz MatA = new MinhaMatriz(Aval);
    	MinhaMatriz MatB = new MinhaMatriz(Bval);
   
    	MatA.prod(MatB).print();
    	
    	System.out.println("");
    	

    	
    }
}

